{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": [
      "error", {
         "devDependencies": true
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
    "import/resolver": {
      "node": {}
    }
  },
  "env": {
    "browser": true,
    "node": true
  }
}