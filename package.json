{
  "name": "facebook-clone-application",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k \"npm run start:client\" \"npm run mock:server\"",
    "start:client": "webpack-dev-server --open --config=./config/webpack.dev.js",
    "build": "webpack --config=./config/webpack.prod.js",
    "mock:server": "node mock-server/index",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint --fix . --ext .js,.jsx",
    "prettier": "prettier --write \"./src/**/*.+(js|jsx|scss)\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.2",
    "css-loader": "^3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "json-server": "^0.15.1",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "connected-react-router": "^6.6.1",
    "history": "^4.10.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "redux": "^4.0.5"
  }
}
